<Window x:Class="PadFootingDesigner.Windows.ConcreteSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PadFootingDesigner.Windows"
        xmlns:ctrl="clr-namespace:PadFootingDesigner.Controls.MaterialsControl"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="ConcreteSelectionWindow" Height="450" Width="800">
    <Window.Resources>
        <ctrl:ConcreteSelectionView x:Key="concreteSelectionView"/>
    </Window.Resources>
    <Grid>
        <ContentControl Content="{StaticResource concreteSelectionView}"/>
    </Grid>
</Window>
